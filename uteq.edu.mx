    <script type="text/javascript">



        $(window).load(function () {



            $(window).resize(function () {

                arrows();

            }).resize();

                       

            function arrows() {

                pageWidth = $(window).width();

                mainWidth = $('#momentum')[0].scrollWidth;

                mainLeft = $('#momentum').scrollLeft();



             /*   if (mainLeft > 0) 

                { $('#leftArrow').fadeIn(); } else { $('#leftArrow').fadeOut(); }*/



             /*   if ((mainWidth - mainLeft) <= pageWidth)

                { $('#rightArrow').fadeOut(); } else { $('#rightArrow').fadeIn(); }*/



                $('#leftArrow').fadeIn();

                $('#rightArrow').fadeIn();

            }



          

            $('#leftArrow').click(function () {

                scrollLeft = $('#momentum').scrollLeft();

                pageWidth = $(window).width() / 2;

                $('#momentum').animate({ scrollLeft: scrollLeft - pageWidth }, 500, function () {

                    arrows();

                });

            });



            $('#rightArrow').click(function () {

                scrollLeft = $('#momentum').scrollLeft();

                pageWidth = $(window).width() / 2;

                $('#momentum').animate({ scrollLeft: scrollLeft + pageWidth }, 500, function () {

                    arrows();

                });

            });



            $('#momentum').on('scroll', function () {

                arrows()

            });

                 

            momentum = document.getElementById("menuHolder");

            momentum.addEventListener('mousedown', startMain, false);



            function startMain(event) {

                event.preventDefault();

                momentum.addEventListener('mousemove', moveMain, false);

                momentum.addEventListener('mouseleave', endMain, false);

                momentum.addEventListener('mouseup', endMain, false);

                distanceX = distanceY = initialX = presentX = 0;

                initialX = event.pageX;

                leftPos = $('#momentum').scrollLeft();

            }

            function moveMain(event) {

                momentum.removeEventListener('mousedown', startMain, false);

                event.preventDefault();

                presentX = event.pageX;

                distanceX = parseInt((presentX - initialX));

                abX = Math.abs(distanceX)

                $('#momentum').scrollLeft(leftPos - distanceX);

            }

            function endMain() {

                momentum.removeEventListener('mousemove', moveMain, false);

                momentum.removeEventListener('mouseleave', endMain, false);

                momentum.removeEventListener('mouseup', endMain, false);

                scrollLeft = $('#momentum').scrollLeft();

                pageWidth = $(window).width() / 2;

                if (distanceX > 0) {

                    $('#momentum').animate({ scrollLeft: scrollLeft - pageWidth }, 500, function () {

                        momentum.addEventListener('mousedown', startMain, false);

                    });

                }

                if (distanceX < 0) {

                    $('#momentum').animate({ scrollLeft: scrollLeft + pageWidth }, 500, function () {

                        momentum.addEventListener('mousedown', startMain, false);

                    });

                }

                if (distanceX === 0) {

                    momentum.addEventListener('mousedown', startMain, false);

                }

            }



            $('#momentum li a').click(function (e) {

                if ($('#momentum').is(':animated')) { e.preventDefault(); }

            });



        });

              

        $(document).ready(function () {



           

            function changeColor() {





                scrollLeft = $('#momentum').scrollLeft();

                pageWidth = $(window).width() / 2;



                if (scrollLeft < 1581) {



                    $('#momentum').animate({ scrollLeft: scrollLeft + pageWidth }, 900, function () {

                        // arrows();

                    });

                }

                else

                {



                    scrollLeft = $('#momentum').scrollLeft();

                    pageWidth = $(window).width() / 2;

                    $('#momentum').animate({ scrollLeft: 0 }, 1581, function () {

                      //  arrows();

                    });



                }

            }



            setInterval(changeColor, 12500);

        });



    </script>   



